alt - q : skhd -r

# cmd - return : open -na /Applications/Alacritty.app
cmd - return : yabai -m window --swap largest

# floating windows
alt - 0x21 : yabai -m window --grid 1:2:0:0:1:1
alt - 0x1E : yabai -m window --grid 1:2:1:0:1:1
alt - 0x1B : yabai -m window --grid 10:10:1:1:8:8
alt - 0x18 : yabai -m window --grid 1:1:0:0:1:1

alt - return : yabai -m window --toggle zoom-parent
# native-fullscreen
shift + alt - return : yabai -m window --toggle zoom-fullscreen
alt - tab : yabai -m window --toggle split

# alt - r : yabai -m space --rotate 90
# shift + alt - r : yabai -m space --rotate 270
alt - t : yabai -m window --toggle float && yabai -m window --grid 4:4:1:1:2:2
alt - o : yabai -m window --toggle topmost
# alt - space : yabai -m space --layout $(yabai -m query --spaces --space | jq -r 'if .type == "bsp" then "float" else "bsp" end')
