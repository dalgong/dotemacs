alt - q : skhd -r

# floating windows
# []
        alt - 0x21 : [[ $(yabai -m query --windows | jq .[0].floating) -eq 0 ]] && \
                     yabai -m window --focus west || yabai -m window --grid 1:2:0:0:1:1
        alt - 0x1E : [[ $(yabai -m query --windows | jq .[0].floating) -eq 0 ]] && \
                     yabai -m window --focus east || yabai -m window --grid 1:2:1:0:1:1
shift + alt - 0x21 : yabai -m window --warp west
shift + alt - 0x1E : yabai -m window --warp east

# -=
        alt - 0x1B : [[ $(yabai -m query --windows | jq .[0].floating) -eq 0 ]] && \
                     yabai -m window --resize right:-20:0 || yabai -m window --grid 10:10:1:1:8:8
shift + alt - 0x1B : [[ $(yabai -m query --windows | jq .[0].floating) -eq 0 ]] && \
                     yabai -m window --resize right:20:0  || yabai -m window --grid 10:10:1:1:8:8
        alt - 0x18 : [[ $(yabai -m query --windows | jq .[0].floating) -eq 0 ]] && \
                     yabai -m window --resize bottom:0:20  || yabai -m window --grid 1:1:0:0:1:1
shift + alt - 0x18 : [[ $(yabai -m query --windows | jq .[0].floating) -eq 0 ]] && \
                     yabai -m window --resize bottom:0:-20 || yabai -m window --grid 1:1:0:0:1:1

# <>.
        alt - 0x2B : yabai -m space --focus prev
        alt - 0x2F : yabai -m space --focus next
        alt - 0x2C : yabai -m space --focus recent
shift + alt - 0x2B : yabai -m window --space prev   && yabai -m space --focus prev  
shift + alt - 0x2F : yabai -m window --space next   && yabai -m space --focus next  
shift + alt - 0x2C : yabai -m window --space recent && yabai -m space --focus recent

        alt - return : yabai -m window --swap largest
        alt - space  : yabai -m window --toggle zoom-fullscreen
shift + alt - space  : yabai -m window --toggle native-fullscreen
        alt - tab    : yabai -m window --toggle zoom-parent
shift + alt - tab    : yabai -m window --toggle split

alt - t : yabai -m window --toggle float && yabai -m window --grid 4:4:1:1:2:2
alt - o : yabai -m window --toggle topmost

# backtick
alt - 0x32 : yabai -m space --layout \
             $(yabai -m query --spaces --space | jq -r 'if .type == "bsp" then "float" else "bsp" end')

alt - p : yabai -m window --focus prev
alt - n : yabai -m window --focus next
shift + alt - p : yabai -m window --warp prev
shift + alt - n : yabai -m window --warp next
