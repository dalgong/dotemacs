alt - q : skhd -r

# floating windows
alt - 0x21 : yabai -m window --grid 1:2:0:0:1:1
alt - 0x1E : yabai -m window --grid 1:2:1:0:1:1
alt - 0x1B : yabai -m window --grid 10:10:1:1:8:8
alt - 0x18 : yabai -m window --grid 1:1:0:0:1:1

shift + alt - 0x21 : yabai -m window --resize right:-20:0
shift + alt - 0x1E : yabai -m window --resize right:20:0

        alt - return : yabai -m window --swap largest
        alt - space  : yabai -m window --toggle zoom-fullscreen
shift + alt - space  : yabai -m window --toggle native-fullscreen
        alt - tab    : yabai -m window --toggle zoom-parent
shift + alt - tab    : yabai -m window --toggle split

# alt - r : yabai -m space --rotate 90
# shift + alt - r : yabai -m space --rotate 270
alt - t : yabai -m window --toggle float && yabai -m window --grid 4:4:1:1:2:2
alt - o : yabai -m window --toggle topmost
shift + alt - backspace : yabai -m space --layout $(yabai -m query --spaces --space | jq -r 'if .type == "bsp" then "float" else "bsp" end')

alt - p : yabai -m window --focus prev
alt - n : yabai -m window --focus next
shift + alt - p : yabai -m window --warp prev
shift + alt - n : yabai -m window --warp next
